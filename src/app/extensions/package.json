{
	"name": "__core_extension__",
	"version": "1.0.0",

	"file_types": [
		{
			"name": "less",
			"version": "1.0.0",
			"extensions": "less",
			"icons": "../assets/img/filetypes/less.png",

			"display": {
				"name": "LESS",
				"extensions": "LESS"
			}
		},
		{
			"name": "sass",
			"version": "1.0.0",
			"extensions": ["sass", "scss"],
			"icons": ["../assets/img/filetypes/sass.png", "../assets/img/filetypes/scss.png"],

			"display": {
				"name": "SASS",
				"extensions": ["SASS", "SCSS"]
			}
		},
		{
			"name": "coffeescript",
			"version": "1.0.0",
			"extensions": "coffee",
			"icons": "../assets/img/filetypes/coffee.png",

			"display": {
				"name": "CoffeeScript",
				"extensions": "CoffeeScript"
			}
		},
		{
			"name": "dust",
			"version": "1.0.0",
			"extensions": "dust",
			"icons": "../assets/img/filetypes/dust.png",

			"display": {
				"name": "Dust",
				"extensions": "Dust"
			}
		}
	],

	"compilers": [
		{
			"name": "less",
			"version": "1.0.0",
			"compiler_version": "1.4.0",
			"class_path": "LessCompiler",
			"fileTypes": "less",
			"output_extensions": "css",

			"options": ["lineComments", "debugInfo", "strictMath", "strictUnits"],
			"outputStyle": ["", "compress", "yuicompress"],

			"display": {
				"name": "LESS",
				"options": ["line comments", "debug info", "Strict Math", "Strict Units"],
				"outputStyle": ["normal", "compress", "yuicompress"]
			},

			"defaults": {
				"options": {
					"lineComments": false,
					"debugInfo": false,
					"strictMath": true,
					"strictUnits": false
				},
				"outputStyle": "",
				"useSystemCommand": false
			}
		},
		{
			"name": "sass",
			"version": "1.0.0",
			"compiler_version": "3.2.9",
			"class_path": "SassCompiler",
			"fileTypes": "sass",
			"output_extensions": "css",

			"options": ["compass", "lineComments", "unixNewlines", "debugInfo"],
			"outputStyle": ["nested", "expanded", "compact", "compressed"],

			"display": {
				"name": "SASS",
				"options": ["compass", "line comments", "unix newlines", "debug info"]
			},

			"defaults": {
				"options": {
					"unixNewlines": false,
					"lineComments": false,
					"debugInfo": false
				},
				"outputStyle": "nested",
				"useSystemCommand": false
			}
		},
		{
			"name": "coffeescript",
			"version": "1.0.0",
			"compiler_version": "1.6.3",
			"class_path": "CoffeeScriptCompiler",
			"fileTypes": "coffeescript",
			"output_extensions": "js",

			"options": ["bare", "literate"],

			"display": {
				"name": "CoffeeScript"
			},

			"defaults": {
				"options": {
					"bare": false,
					"literate": false
				},
				"useSystemCommand": false
			}
		},
		{
			"name": "dust",
			"version": "1.0.0",
			"compiler_version": "1.2.3",
			"class_path": "DustCompiler",
			"fileTypes": "dust",
			"output_extensions": "jst",

			"display": {
				"name": "Dust"
			},

			"defaults": {
				"useSystemCommand": false
			}
		}
	]
}